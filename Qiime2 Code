qiime picrust2 full-pipeline \
   --i-table day7_table.qza \
   --i-seq rep-seqs.qza \
   --output-dir q2-picrust2_output \
   --p-placement-tool sepp \
   --p-threads 1 \
   --p-hsp-method pic \
   --p-max-nsti 2 \
   --verbose

mkdir picrust2_day21
cd picrust2_day21
